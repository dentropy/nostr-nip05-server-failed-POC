{
    "app_names" : ["nostr-nip05-server"],
    "authors"   : ["dentropy@ddaemon.org"],
    "schema" : {
        "root" : {
            "key_value_patterns" : [
                "namespace_to_ipns",
                "change_log_namespace_to_ipns_index_*",
                "change_log_namespace_to_ipns_count"
            ],
            "load_defaults" : {
                "namespace_to_ipns"  : {
                    "admin_identities" : "${IPNS_NAME}",
                    "nostr_identities" : "${IPNS_NAME}",
                    "dns_names" : "${IPNS_NAME}",
                    "nostr_dot_json_jq_changes" : "${IPNS_NAME}",
                    "coupons" : "${IPNS_NAME}",
                    "offers" : "${IPNS_NAME}",
                    "nostr_relays" : "${IPNS_NAME}",
                    "purchases" : "${IPNS_NAME}"
                },
                "change_log_namespace_to_ipns_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "admin_identities" : {
            "key_value_patterns" : [
                "admin_npub_key_*",
                "change_log_index_*",
                "change_log_count"
            ],
            "load_defaults" : {
                "change_log_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "nostr_identities" : {
            "key_value_patterns" : [
                "user_npub_key_*",
                "change_log_index_*",
                "change_log_count"
            ],
            "load_defaults" : {
                "change_log_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "dns_names" : {
            "key_value_patterns" : [
                "all_dns_names",
                "public_dns_names",
                "default_dns_name",
                "change_log_all_dns_names_json_jq_count",
                "change_log_all_dns_names_json_jq_index_*",
                "change_log_public_dns_names_jq_count",
                "change_log_public_dns_names_jq_index_"
            ],
            "load_defaults" : {
                "change_log_all_dns_names_json_jq_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                },
                "change_log_public_dns_names_jq_count"   : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "nostr_dot_json_jq_changes" : {
            "key_value_patterns" : [
                "nostr_json_${DNS_NAME}",
                "change_log_${DNS_NAME}_jq_index_${ZERO_PADDED_CHANGE_NUMBER}",
                "change_log_${DNS_NAME}_jq_count"
            ],
            "load_defaults" : {}
        },
        "coupons" : {
            "key_value_patterns" : [
                "coupon_code_${COUPON_CODE}",
                "coupon_valid_${COUPON_CODE}",
                "coupon_claimed_${COUPON_CODE}_${ZERO_PADDED_CLAIM_NUMBER}",
                "coupon_removed_${COUPON_CODE}",
                "change_log_${ZERO_PADDED_CHANGE_NUMBER}",
                "change_log_count"
            ],
            "load_defaults" : {
                "change_log_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "offers" : {
            "key_value_patterns" : [
                "offer_${DNS_NAME}",
                "coupon_valid_${COUPON_CODE}",
                "change_log_index_${ZERO_PADDED_CHANGE_NUMBER}_${ZERO_PADDED_CLAIM_NUMBER}",
                "change_log_count"
            ],
            "load_defaults" : {
                "change_log_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "nostr_relays" : {
            "key_value_patterns" : [
                "nostr_relay_domain_${WEBSOCKET_TYPE}_${RELAY_DOMAIN_NAME}",
                "change_log_${ZERO_PADDED_CHANGE_NUMBER}",
                "change_log_count"
            ],
            "load_defaults" : {
                "change_log_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        },
        "purchases" : {
            "key_value_patterns" : [
                "nostr_relay_domain_${WEBSOCKET_TYPE}_${RELAY_DOMAIN_NAME}",
                "purchase_${PURCHASE_NUM}",
                "hange_log_${ZERO_PADDED_CHANGE_NUMBER}",
                "change_log_count"
            ],
            "load_defaults" : {
                "change_log_count" : {
                    "type" : "raw_value_0.0.1",
                    "value" : 0
                }
            }
        }
    }
}