{
    "app_names": [
        "nostr_NIP05_server"
    ],
    "tags": "nostr",
    "authors": [
        "dentropy@ddaemon.org"
    ],
    "dependencies": [
        {
            "name": "RBAC.root_RBAC"
        },
        {
            "name": "apps.nostr_NIP05_relay_map"
        }
    ],
    "schema_variables": {
        "NOSTR_PUBLIC_KEY": {
            "Description": "Just take the npub off the nostr public key"
        },
        "LOG_AUTO_INCREMENT": {
            "Description": "Increments with the log going up"
        }
    },
    "schema": {
        "logs": {
            "key_value_patterns": [
                "logs_${SOME_VARIABLE}",
                "change_log_index_${LOG_AUTO_INCREMENT}",
                "change_log_count"
            ],
            "load_defaults": {
                "change_log_count" : {
                    "type" : "json_value",
                    "version": "0.0.1",
                    "value" : { "count" : 0 }
                }
            }
        }
    }
}